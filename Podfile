# platform :ios, '9.0'

target 'TemperJSearch' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TemperJSearch
 	pod 'RxSwift', '~> 5'
  	pod 'RxCocoa', '~> 5'
	pod 'SnapKit', '~> 5.0.0'
	pod 'Kingfisher', '~> 5.0' # for image caching
	pod 'NVActivityIndicatorView'
	pod 'Alamofire', '~> 5.0.0-beta.5'

  target 'TemperJSearchTests' do
    inherit! :search_paths
    # Pods for testing
	pod 'RxBlocking', '~> 5'
    	pod 'RxTest', '~> 5'
  end

  target 'TemperJSearchUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
